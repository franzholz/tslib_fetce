{
  "name": "jambagecom/tslib-fetce",
  "type": "typo3-cms-extension",
  "description": "This extension brings the TYPO3 4.x class tslib_feTCE and the processScript FEData setup with new features partly back into TYPO3 9 and later",
  "keywords": [
    "TYPO3",
    "extension",
    "tslib_feTCE"
  ],
  "support": {
    "issues": "https://github.com/franzholz/tslib_fetce/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^12 || ^13",
    "jambagecom/div2007": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\TslibFetce\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/tslib-fetce": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tslib_fetce"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.44"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
